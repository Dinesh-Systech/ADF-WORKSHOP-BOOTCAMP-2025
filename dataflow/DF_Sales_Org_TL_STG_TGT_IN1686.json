{
	"name": "DF_Sales_Org_TL_STG_TGT_IN1686",
	"properties": {
		"folder": {
			"name": "Final_Project/STG_TGT"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_AzureSqlTable_TGT_IN1686",
						"type": "DatasetReference"
					},
					"name": "SRCSalesOrgTL"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_AzureSqlTable_TGT_IN1686",
						"type": "DatasetReference"
					},
					"name": "SNKAlcSalesOrgTL"
				}
			],
			"transformations": [
				{
					"name": "SKSalesOrgTL"
				},
				{
					"name": "NullHandling"
				},
				{
					"name": "select1"
				},
				{
					"name": "DataTypeConversion"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {TL Territory Name} as string,",
				"          {TL Territory Code} as string,",
				"          {TL Territory ID} as integer,",
				"          TL as string,",
				"          {TL SFA Code} as string,",
				"          {TL Code} as integer,",
				"          {TL Email} as string,",
				"          {TL Mobile} as long,",
				"          {Market Working Norms} as integer,",
				"          {Min Market Working with TSE} as integer,",
				"          Active_flag as string,",
				"          {TL Start Date} as string,",
				"          {TL End Date} as string,",
				"          Cluster_code as string,",
				"          {Created By} as string,",
				"          {Created Date} as timestamp,",
				"          {Updated By} as string,",
				"          {Updated Date} as timestamp,",
				"          Pipeline_RunID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SRCSalesOrgTL",
				"NullHandling keyGenerate(output(TL_Key as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> SKSalesOrgTL",
				"DataTypeConversion derive(each(match(type=='integer'), $$ = iifNull($#,0)),",
				"          each(match(type=='string'), $$ = replace(iifNull($$,'UNK'),'null','UNK')),",
				"          each(match(type=='timestamp'), $$ = iifNull($$,toTimestamp('31/12/2999 00:00:00'))),",
				"          each(match(type=='date'), $$ = iifNull($$,toDate('31/12/2999')))) ~> NullHandling",
				"SKSalesOrgTL select(mapColumn(",
				"          TL_Key,",
				"          {TL Territory Name},",
				"          {TL Territory Code},",
				"          {TL Territory ID},",
				"          TL,",
				"          {TL SFA Code},",
				"          {TL Code},",
				"          {TL Email},",
				"          {TL Mobile},",
				"          {Market Working Norms},",
				"          {Min Market Working with TSE},",
				"          Active_flag,",
				"          {TL Start Date},",
				"          {TL End Date},",
				"          Cluster_code,",
				"          {Created By},",
				"          {Created Date},",
				"          {Updated By},",
				"          {Updated Date},",
				"          Pipeline_RunID",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"SRCSalesOrgTL derive({TL Start Date} = toDate({TL Start Date},'dd-MM-yyyy'),",
				"          {TL End Date} = toDate({TL End Date},'dd-MM-yyyy')) ~> DataTypeConversion",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          PromotionCode as string,",
				"          PromotionDescription as string,",
				"          PromotionType as string,",
				"          RetailerCode as string,",
				"          EffectiveFrom as date,",
				"          EffectiveTo as date,",
				"          MappingLevel as string,",
				"          IsActive as string,",
				"          {Created By} as string,",
				"          {Created Date} as timestamp,",
				"          {Updated By} as string,",
				"          {Updated Date} as timestamp",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> SNKAlcSalesOrgTL"
			]
		}
	}
}