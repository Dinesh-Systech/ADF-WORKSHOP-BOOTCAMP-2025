				},
				{
					"dataset": {
						"referenceName": "DS_AzureSql_IN1684",
						"type": "DatasetReference"
					},
					"name": "SRCGeography"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_AzureSql_IN1684",
						"type": "DatasetReference"
					},
					"name": "SNKPrimaryActuals"
				}
			],
			"transformations": [
				{
					"name": "leftPrimaryActualsAndCompany"
				},
				{
					"name": "leftPlant"
				},
				{
					"name": "leftProduct"
				},
				{
					"name": "leftGeograpghy"
				},
