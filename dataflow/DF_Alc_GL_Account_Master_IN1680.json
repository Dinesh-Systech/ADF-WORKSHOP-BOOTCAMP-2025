				"     startAt: 1L,",
				"     stepValue: 1L) ~> StateKey",
				"NullHandle select(mapColumn(",
				"          GeneralLedgerCode,",
				"          GeneralLedgerName,",
				"          GLType,",
				"          Currency,",
				"          Reporting_Hierarchy_Name_L1,",
				"          Reporting_Hierarchy_Name_L2,",
				"          Reporting_Hierarchy_Name_L3,",
				"          Reporting_Hierarchy_Name_L4,",
				"          CreatedAt,",
				"          UpdatedAt,",
				"          CreatedBy,",
				"          UpdatedBy",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SLCTcols",
				"StateKey sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
