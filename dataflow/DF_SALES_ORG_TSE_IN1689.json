{
	"name": "DF_SALES_ORG_TSE_IN1689",
	"properties": {
		"folder": {
			"name": "END TO END WORKSHOP/STG TO DW"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_AzureSQLtable_TGT_IN1689",
						"type": "DatasetReference"
					},
					"name": "SrcSalesTSEMaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_AzureSQLtable_TGT_IN1689",
						"type": "DatasetReference"
					},
					"name": "TgtSalesTSEMaster"
				}
			],
			"transformations": [
				{
					"name": "TSEKey"
				},
				{
					"name": "NullHandling"
				},
				{
					"name": "DataTypeConversion"
				}
			],
			"scriptLines": [
				"source(output(",
				"          TSE_Territory_Name as string,",
				"          TSE_Territory_Code as string,",
				"          TSE_Territory_ID as integer,",
				"          TSE as string,",
				"          TSE_Code as integer,",
				"          TSE_SFA_Code as string,",
				"          TSE_Email as string,",
				"          TSE_Mobile as long,",
				"          TSE_Call_Norm as integer,",
				"          State as string,",
				"          State_Code as string,",
				"          DISTRICT as string,",
				"          DISTRICT_CODE as string,",
				"          Active_Flag as integer,",
				"          TSE_Start_Date as string,",
				"          TSE_End_Date as string,",
				"          TL_Code as string,",
				"          CreatedAt as timestamp,",
				"          UpdatedAt as timestamp,",
				"          CreatedBy as string,",
				"          UpdatedBy as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SrcSalesTSEMaster",
				"DataTypeConversion keyGenerate(output(ClusterKey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> TSEKey",
				"TSEKey derive(each(match(type=='string'), $$ = iif(isNull($$) || $$ == 'null'|| trim($$) == '', 'UNK', $$)),",
				"          each(match(type=='timestamp'), $$ = iifNull($$, toTimestamp('2999-12-31 00:00:00', 'yyyy-MM-dd HH:mm:ss'))),",
				"          each(match(type=='integer'), $$ = iifNull($$, 0 ))) ~> NullHandling",
				"SrcSalesTSEMaster derive(TSE_Start_Date = toDate(TSE_Start_Date,\"dd-MM-yyyy\"),",
				"          TSE_End_Date = toDate(TSE_End_Date,\"dd-MM-yyyy\")) ~> DataTypeConversion",
				"NullHandling sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          TSE_Territory_Name,",
				"          TSE_Territory_Code,",
				"          TSE_Territory_Key = ClusterKey,",
				"          TSE_Territory_ID,",
				"          TSE,",
				"          TSE_Code,",
				"          TSE_SFA_Code,",
				"          TSE_Email,",
				"          TSE_Mobile,",
				"          TSE_Call_Norm,",
				"          State,",
				"          State_Code,",
				"          DISTRICT,",
				"          DISTRICT_CODE,",
				"          Active_Flag,",
				"          TSE_Start_Date,",
				"          TSE_End_Date,",
				"          TL_Code,",
				"          CreatedAt,",
				"          UpdatedAt,",
				"          CreatedBy,",
				"          UpdatedBy,",
				"          ClusterKey",
				"     )) ~> TgtSalesTSEMaster"
			]
		}
	}
}