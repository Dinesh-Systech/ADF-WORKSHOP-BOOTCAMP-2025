{
	"name": "DF_SALES_ORG_CLUSTER_IN1689",
	"properties": {
		"folder": {
			"name": "END TO END WORKSHOP/STG TO DW"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_AzureSQLtable_TGT_IN1689",
						"type": "DatasetReference"
					},
					"name": "SrcSalesClusterMaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_AzureSQLtable_TGT_IN1689",
						"type": "DatasetReference"
					},
					"name": "TgtSalesClusterMaster"
				}
			],
			"transformations": [
				{
					"name": "ClusterKey"
				},
				{
					"name": "NullHandling"
				},
				{
					"name": "DataTypeConversion"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Cluster_Name as string,",
				"          Cluster_Code as string,",
				"          Cluster_ID as integer,",
				"          Cluster_Head as string,",
				"          Cluster_SFA_Code as string,",
				"          Cluster_Head_Email as string,",
				"          Cluster__Head_Mobile as long,",
				"          Active_flag as integer,",
				"          Cluster_Start_Date as string,",
				"          Cluster_End_Date as string,",
				"          CreatedAt as timestamp,",
				"          UpdatedAt as timestamp,",
				"          CreatedBy as string,",
				"          UpdatedBy as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SrcSalesClusterMaster",
				"DataTypeConversion keyGenerate(output(ClusterKey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> ClusterKey",
				"ClusterKey derive(each(match(type=='string'), $$ = iif(isNull($$) || $$ == 'null'|| trim($$) == '', 'UNK', $$)),",
				"          each(match(type=='timestamp'), $$ = iifNull($$, toTimestamp('2999-12-31 00:00:00', 'yyyy-MM-dd HH:mm:ss'))),",
				"          each(match(type=='integer'), $$ = iifNull($$, 0 ))) ~> NullHandling",
				"SrcSalesClusterMaster derive(Cluster_Start_Date = toDate(Cluster_Start_Date,\"dd-MM-yyyy\"),",
				"          Cluster_End_Date = toDate(Cluster_End_Date,\"dd-MM-yyyy\")) ~> DataTypeConversion",
				"NullHandling sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          Cluster_Name,",
				"          Cluster_Code,",
				"          Cluster_ID,",
				"          Cluster_Head,",
				"          Cluster_SFA_Code,",
				"          Cluster_Head_Email,",
				"          Cluster_Head_Mobile = Cluster__Head_Mobile,",
				"          Active_flag,",
				"          Cluster_Start_Date,",
				"          Cluster_End_Date,",
				"          CreatedAt,",
				"          UpdatedAt,",
				"          CreatedBy,",
				"          UpdatedBy,",
				"          Cluster_Key = ClusterKey",
				"     )) ~> TgtSalesClusterMaster"
			]
		}
	}
}