{
	"name": "DF_OrgTL_IN1674",
	"properties": {
		"folder": {
			"name": "Final_Project/STG_TGT"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_AzureSqlTarget_IN1674",
						"type": "DatasetReference"
					},
					"name": "SrcGLAccountMaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_AzureSqlTarget_IN1674",
						"type": "DatasetReference"
					},
					"name": "TgtGLAccountMaster"
				}
			],
			"transformations": [
				{
					"name": "Generalkey"
				},
				{
					"name": "NullHandling"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {TL Territory Name} as string,",
				"          {TL Territory Code} as string,",
				"          {TL Territory ID} as string,",
				"          TL as string,",
				"          {TL SFA Code} as string,",
				"          {TL Code} as string,",
				"          {TL Email} as string,",
				"          {TL Mobile} as string,",
				"          {Market Working Norms} as string,",
				"          {Min Market Working with TSE} as string,",
				"          Active_flag as string,",
				"          {TL Start Date} as string,",
				"          {TL End Date} as string,",
				"          Cluster_code as string,",
				"          CreatedAt as date,",
				"          UpdatedAt as date,",
				"          CreatedBy as string,",
				"          UpdatedBy as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SrcGLAccountMaster",
				"SrcGLAccountMaster keyGenerate(output(TLKey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> Generalkey",
				"Generalkey derive(each(match(type=='string'), $$ = iif(isNull($$) || $$ == 'NULL', 'UNK', $$)),",
				"          each(match(type=='timestamp'), $$ = iifNull($$, toTimestamp('2999-12-31 00:00:00', 'yyyy-MM-dd HH:mm:ss'))),",
				"          UpdatedAt = currentDate()) ~> NullHandling",
				"NullHandling select(mapColumn(",
				"          {TL Territory Name},",
				"          {TL Territory Code},",
				"          {TL Territory ID},",
				"          TL,",
				"          {TL SFA Code},",
				"          {TL Code},",
				"          {TL Email},",
				"          {TL Mobile},",
				"          {Market Working Norms},",
				"          {Min Market Working with TSE},",
				"          Active_flag,",
				"          {TL Start Date},",
				"          {TL End Date},",
				"          Cluster_code,",
				"          CreatedAt,",
				"          UpdatedAt,",
				"          CreatedBy,",
				"          UpdatedBy,",
				"          TLKey",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> TgtGLAccountMaster"
			]
		}
	}
}