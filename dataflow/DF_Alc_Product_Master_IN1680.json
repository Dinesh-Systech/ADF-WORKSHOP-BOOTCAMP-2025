{
	"name": "DF_Alc_Product_Master_IN1680",
	"properties": {
		"folder": {
			"name": "stg - dw"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Azure_sql_IN1680",
						"type": "DatasetReference"
					},
					"name": "SRCstgproduct"
				},
				{
					"dataset": {
						"referenceName": "DS_Azure_sql_IN1680",
						"type": "DatasetReference"
					},
					"name": "SRCtgtProduct"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_Azure_sql_IN1680",
						"type": "DatasetReference"
					},
					"name": "SNKtgtProduct"
				},
				{
					"dataset": {
						"referenceName": "DS_Azure_sql_IN1680",
						"type": "DatasetReference"
					},
					"name": "SNKtgtUpdate"
				}
			],
			"transformations": [
				{
					"name": "selectTgt"
				},
				{
					"name": "LeftJoinStgAndTgt"
				},
				{
					"name": "insert"
				},
				{
					"name": "updatedAt"
				},
				{
					"name": "SLCTsrccols"
				},
				{
					"name": "SLCTtgtUpdate"
				},
				{
					"name": "NullHandling"
				},
				{
					"name": "AlterUpdate"
				}
			],
			"scriptLines": [
				"source(output(",
				"          SKUCode as integer,",
				"          ItemName as string,",
				"          CompanyCode as string,",
				"          IndustryName as string,",
				"          BrandFamily as string,",
				"          ReportingSegment as string,",
				"          BrandPackSize as string,",
				"          PackTypeCode as string,",
				"          BrandPackType as string,",
				"          State_Code as string,",
				"          State as string,",
				"          BrandCode as string,",
				"          BrandName as string,",
				"          ProductCategory as string,",
				"          ProductSegment as string,",
				"          CaseConfig as integer,",
				"          Division as string,",
				"          ConversionFactor as integer,",
				"          AcquiredStartDate as timestamp,",
				"          AcquiredEndDate as timestamp,",
				"          DisposedStartDate as timestamp,",
				"          DisposedEndDate as timestamp,",
				"          InnovationFlag as string,",
				"          Flavour as string,",
				"          SubFlavour as string,",
				"          CreatedAt as timestamp,",
				"          UpdatedAt as timestamp,",
				"          CreatedBy as string,",
				"          UpdatedBy as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SRCstgproduct",
				"source(output(",
				"          SKUCode as integer,",
				"          ItemKey as integer,",
				"          ItemName as string,",
				"          CompanyCode as string,",
				"          IndustryName as string,",
				"          BrandFamily as string,",
				"          ReportingSegment as string,",
				"          BrandPackSize as string,",
				"          PackTypeCode as string,",
				"          BrandPackType as string,",
				"          State_Code as string,",
				"          State as string,",
				"          BrandCode as string,",
				"          BrandName as string,",
				"          ProductCategory as string,",
				"          ProductSegment as string,",
				"          CaseConfig as integer,",
				"          Division as string,",
				"          ConversionFactor as integer,",
				"          AcquiredStartDate as timestamp,",
				"          AcquiredEndDate as timestamp,",
				"          DisposedStartDate as timestamp,",
				"          DisposedEndDate as timestamp,",
				"          InnovationFlag as string,",
				"          Flavour as string,",
				"          SubFlavour as string,",
				"          CreatedAt as timestamp,",
				"          UpdatedAt as timestamp,",
				"          CreatedBy as string,",
				"          UpdatedBy as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SRCtgtProduct",
				"SRCtgtProduct select(mapColumn(",
				"          each(match(true()),",
				"               'tgt_'+$$ = $$)",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectTgt",
				"SRCstgproduct, selectTgt join(SKUCode == tgt_SKUCode,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> LeftJoinStgAndTgt",
				"LeftJoinStgAndTgt split(isNull(tgt_SKUCode),",
				"     !isNull(tgt_SKUCode) && (tgt_ReportingSegment!=ReportingSegment),",
				"     disjoint: true) ~> insert@(insert, update)",
				"SLCTtgtUpdate derive(UpdatedAt = currentTimestamp(),",
				"          UpdatedBy = 'PL_Alc_Product_Master_IN1680') ~> updatedAt",
				"NullHandling select(mapColumn(",
				"          SKUCode,",
				"          ItemName = Flavour,",
				"          CompanyCode,",
				"          IndustryName,",
				"          BrandFamily,",
				"          ReportingSegment,",
				"          BrandPackSize,",
				"          PackTypeCode,",
				"          BrandPackType,",
				"          State_Code,",
				"          State,",
				"          BrandCode,",
				"          BrandName,",
				"          ProductCategory,",
				"          ProductSegment,",
				"          CaseConfig,",
				"          Division,",
				"          ConversionFactor,",
				"          AcquiredStartDate,",
				"          AcquiredEndDate,",
				"          DisposedStartDate,",
				"          DisposedEndDate,",
				"          InnovationFlag,",
				"          Flavour,",
				"          SubFlavour,",
				"          CreatedAt,",
				"          UpdatedAt,",
				"          CreatedBy,",
				"          UpdatedBy",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: true) ~> SLCTsrccols",
				"insert@update select(mapColumn(",
				"          ReportingSegment,",
				"          UpdatedAt,",
				"          UpdatedBy,",
				"          SKUCode = tgt_SKUCode,",
				"          ItemName = tgt_ItemName,",
				"          CompanyCode = tgt_CompanyCode,",
				"          IndustryName = tgt_IndustryName,",
				"          BrandFamily = tgt_BrandFamily,",
				"          BrandPackSize = tgt_BrandPackSize,",
				"          PackTypeCode = tgt_PackTypeCode,",
				"          BrandPackType = tgt_BrandPackType,",
				"          State_Code = tgt_State_Code,",
				"          State = tgt_State,",
				"          BrandCode = tgt_BrandCode,",
				"          BrandName = tgt_BrandName,",
				"          ProductCategory = tgt_ProductCategory,",
				"          ProductSegment = tgt_ProductSegment,",
				"          CaseConfig = tgt_CaseConfig,",
				"          Division = tgt_Division,",
				"          ConversionFactor = tgt_ConversionFactor,",
				"          AcquiredStartDate = tgt_AcquiredStartDate,",
				"          AcquiredEndDate = tgt_AcquiredEndDate,",
				"          DisposedStartDate = tgt_DisposedStartDate,",
				"          DisposedEndDate = tgt_DisposedEndDate,",
				"          InnovationFlag = tgt_InnovationFlag,",
				"          Flavour = tgt_Flavour,",
				"          SubFlavour = tgt_SubFlavour,",
				"          CreatedAt = tgt_CreatedAt,",
				"          CreatedBy = tgt_CreatedBy",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SLCTtgtUpdate",
				"insert@insert derive(each(match(type=='string'), $$ = iifNull($$, 'UNK')),",
				"          each(match(type=='integer'), $$ = iifNull($$, -1)),",
				"          each(match(type=='timestamp'), $$ = iifNull($$, toTimestamp('2999-12-31 00:00:00', 'yyyy-MM-dd HH:mm:ss')))) ~> NullHandling",
				"updatedAt alterRow(updateIf(true())) ~> AlterUpdate",
				"SLCTsrccols sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> SNKtgtProduct",
				"AlterUpdate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:false,",
				"     updateable:true,",
				"     upsertable:false,",
				"     keys:['SKUCode'],",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> SNKtgtUpdate"
			]
		}
	}
}