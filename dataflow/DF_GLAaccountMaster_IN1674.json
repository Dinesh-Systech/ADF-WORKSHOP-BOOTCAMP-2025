{
	"name": "DF_GLAaccountMaster_IN1674",
	"properties": {
		"folder": {
			"name": "Final_Project/STG_TGT"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_AzureSqlTarget_IN1674",
						"type": "DatasetReference"
					},
					"name": "SRCAccountMaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_AzureSqlTarget_IN1674",
						"type": "DatasetReference"
					},
					"name": "SNKAlcAccountMaster"
				}
			],
			"transformations": [
				{
					"name": "SKAccountMaster"
				},
				{
					"name": "NullHandling"
				},
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          GeneralLedgerCode as string,",
				"          GeneralLedgerName as string,",
				"          GLType as string,",
				"          Currency as string,",
				"          Reporting_Hierarchy_Name_L1 as string,",
				"          Reporting_Hierarchy_Name_L2 as string,",
				"          Reporting_Hierarchy_Name_L3 as string,",
				"          Reporting_Hierarchy_Name_L4 as string,",
				"          CreatedAt as date,",
				"          UpdatedAt as date,",
				"          CreatedBy as string,",
				"          UpdatedBy as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SRCAccountMaster",
				"NullHandling keyGenerate(output({GeneralLedgerKey } as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> SKAccountMaster",
				"SRCAccountMaster derive(each(match(type=='integer'), $$ = iifNull($#,0)),",
				"          each(match(type=='string'), $$ = replace(iifNull($$,'UNK'),'null','UNK')),",
				"          each(match(type=='timestamp'), $$ = iifNull($$,toTimestamp('31/12/2999 00:00:00'))),",
				"          each(match(type=='datetime'), $$ = iifNull($$,toDate('31/12/1999'))),",
				"          UpdatedAt = currentDate()) ~> NullHandling",
				"SKAccountMaster select(mapColumn(",
				"          GeneralLedgerCode,",
				"          GeneralLedgerName,",
				"          GLType,",
				"          Currency,",
				"          Reporting_Hierarchy_Name_L1,",
				"          Reporting_Hierarchy_Name_L2,",
				"          Reporting_Hierarchy_Name_L3,",
				"          Reporting_Hierarchy_Name_L4,",
				"          CreatedAt,",
				"          UpdatedAt,",
				"          CreatedBy,",
				"          UpdatedBy,",
				"          {GeneralLedgerKey }",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> SNKAlcAccountMaster"
			]
		}
	}
}