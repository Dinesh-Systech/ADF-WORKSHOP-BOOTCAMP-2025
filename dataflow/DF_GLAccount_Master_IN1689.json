{
	"name": "DF_GLAccount_Master_IN1689",
	"properties": {
		"folder": {
			"name": "END TO END WORKSHOP/STG TO DW"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_AzureSQLtable_TGT_IN1689",
						"type": "DatasetReference"
					},
					"name": "SrcGLAccountMaster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_AzureSQLtable_TGT_IN1689",
						"type": "DatasetReference"
					},
					"name": "TgtGLAccountMaster"
				}
			],
			"transformations": [
				{
					"name": "GeneralLedgerKey"
				},
				{
					"name": "NullHandling"
				}
			],
			"scriptLines": [
				"source(output(",
				"          GeneralLedgerCode as integer,",
				"          GeneralLedgerName as string,",
				"          GLType as string,",
				"          Currency as string,",
				"          Reporting_Hierarchy_Name_L1 as string,",
				"          Reporting_Hierarchy_Name_L2 as string,",
				"          Reporting_Hierarchy_Name_L3 as string,",
				"          Reporting_Hierarchy_Name_L4 as string,",
				"          CreatedAt as timestamp,",
				"          UpdatedAt as timestamp,",
				"          CreatedBy as string,",
				"          UpdatedBy as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SrcGLAccountMaster",
				"SrcGLAccountMaster keyGenerate(output(GeneralLedgerKey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> GeneralLedgerKey",
				"GeneralLedgerKey derive(each(match(type=='string'), $$ = iif(isNull($$) || $$ == 'null', 'UNK', $$)),",
				"          each(match(type=='timestamp'), $$ = iifNull($$, toTimestamp('2999-12-31 00:00:00', 'yyyy-MM-dd HH:mm:ss')))) ~> NullHandling",
				"NullHandling sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          GeneralLedgerCode,",
				"          GeneralLedgerName,",
				"          GLType,",
				"          Currency,",
				"          Reporting_Hierarchy_Name_L1,",
				"          Reporting_Hierarchy_Name_L2,",
				"          Reporting_Hierarchy_Name_L3,",
				"          Reporting_Hierarchy_Name_L4,",
				"          CreatedAt,",
				"          UpdatedAt,",
				"          CreatedBy,",
				"          UpdatedBy,",
				"          GeneralLedgerKey",
				"     )) ~> TgtGLAccountMaster"
			]
		}
	}
}