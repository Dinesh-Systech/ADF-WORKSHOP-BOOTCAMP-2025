{
	"name": "DF_SalesOrgCluster_IN1674",
	"properties": {
		"folder": {
			"name": "Final_Project/STG_TGT"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_AzureSqlTarget_IN1674",
						"type": "DatasetReference"
					},
					"name": "SRCSalesOrgCluster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_AzureSqlTarget_IN1674",
						"type": "DatasetReference"
					},
					"name": "SNKAlcSalesOrgCluster"
				}
			],
			"transformations": [
				{
					"name": "SKSalesOrgCluster"
				},
				{
					"name": "NullHandling"
				},
				{
					"name": "select1"
				},
				{
					"name": "DataTypeConversion"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Cluster Name} as string,",
				"          {Cluster Code} as string,",
				"          {Cluster ID} as integer,",
				"          {Cluster Head} as string,",
				"          {Cluster SFA Code} as string,",
				"          {Cluster Head Email} as string,",
				"          {Cluster  Head Mobile} as integer,",
				"          Active_flag as integer,",
				"          {Cluster Start Date} as string,",
				"          {Cluster End Date} as string,",
				"          CreatedAt as date,",
				"          UpdatedAt as date,",
				"          CreatedBy as string,",
				"          UpdatedBy as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SRCSalesOrgCluster",
				"NullHandling keyGenerate(output(Cluster_Key as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> SKSalesOrgCluster",
				"DataTypeConversion derive(each(match(type=='integer'), $$ = iifNull($#,0)),",
				"          each(match(type=='string'), $$ = replace(iifNull($$,'UNK'),'null','UNK')),",
				"          each(match(type=='timestamp'), $$ = iifNull($$,toTimestamp('31/12/2999 00:00:00'))),",
				"          each(match(type=='date'), $$ = iifNull($$,toDate('31/12/2999')))) ~> NullHandling",
				"SKSalesOrgCluster select(mapColumn(",
				"          {Cluster Name},",
				"          {Cluster Code},",
				"          {Cluster ID},",
				"          {Cluster Head},",
				"          {Cluster SFA Code},",
				"          {Cluster Head Email},",
				"          {Cluster  Head Mobile},",
				"          Active_flag,",
				"          {Cluster Start Date},",
				"          {Cluster End Date},",
				"          CreatedAt,",
				"          UpdatedAt,",
				"          CreatedBy,",
				"          UpdatedBy,",
				"          Cluster_Key",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"SRCSalesOrgCluster derive({Cluster Start Date} = toDate({Cluster Start Date},'dd-MM-yyyy'),",
				"          {Cluster End Date} = toDate({Cluster End Date},'dd-MM-yyyy'),",
				"          UpdatedAt = currentDate()) ~> DataTypeConversion",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> SNKAlcSalesOrgCluster"
			]
		}
	}
}