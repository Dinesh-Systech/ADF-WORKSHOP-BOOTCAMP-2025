{
	"name": "DF_SalesOrgCluster_IN1674",
	"properties": {
		"folder": {
			"name": "Final_Project/STG_TGT"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_AzureSqlTarget_IN1674",
						"type": "DatasetReference"
					},
					"name": "SRCStgCluster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_AzureSqlTarget_IN1674",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "NullHandle"
				},
				{
					"name": "KeyCluster"
				},
				{
					"name": "SLCTcols"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Cluster Name} as string,",
				"          {Cluster Code} as string,",
				"          {Cluster ID} as string,",
				"          {Cluster Head} as string,",
				"          {Cluster SFA Code} as string,",
				"          {Cluster Head Email} as string,",
				"          {Cluster  Head Mobile} as string,",
				"          Active_flag as string,",
				"          {Cluster Start Date} as string,",
				"          {Cluster End Date} as string,",
				"          CreatedAt as date,",
				"          UpdatedAt as date,",
				"          CreatedBy as string,",
				"          UpdatedBy as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SRCStgCluster",
				"SRCStgCluster derive(each(match(type=='string'), $$ = iifNull($$,'UNK')),",
				"          each(match(type=='integer'), $$ = iifNull($$,-1)),",
				"          each(match(type=='timestamp'), $$ = iifNull($$,toTimestamp('1900-01-01', 'yyyy-MM-dd'))),",
				"          Cluster_Start_DateN = toDate({Cluster Start Date}, 'dd-MM-yyyy'),",
				"          Cluster_End_DateN = toDate({Cluster End Date}, 'dd-MM-yyyy')) ~> NullHandle",
				"SLCTcols keyGenerate(output(Cluster_Key as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> KeyCluster",
				"NullHandle select(mapColumn(",
				"          {Cluster Name},",
				"          {Cluster Code},",
				"          {Cluster ID},",
				"          {Cluster Head},",
				"          {Cluster SFA Code},",
				"          {Cluster Head Email},",
				"          {Cluster  Head Mobile},",
				"          Active_flag,",
				"          {Cluster Start Date},",
				"          {Cluster End Date},",
				"          CreatedAt,",
				"          UpdatedAt,",
				"          CreatedBy,",
				"          UpdatedBy,",
				"          Cluster_Start_DateN,",
				"          Cluster_End_DateN",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SLCTcols",
				"KeyCluster sink(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          {Cluster Name},",
				"          {Cluster Code},",
				"          {Cluster ID},",
				"          {Cluster Head},",
				"          {Cluster SFA Code},",
				"          {Cluster Head Email},",
				"          {Cluster  Head Mobile},",
				"          Active_flag,",
				"          {Cluster Start Date},",
				"          {Cluster End Date},",
				"          CreatedAt,",
				"          UpdatedAt,",
				"          CreatedBy,",
				"          UpdatedBy,",
				"          Cluster_Start_DateN,",
				"          Cluster_End_DateN,",
				"          Cluster_Key",
				"     )) ~> sink1"
			]
		}
	}
}