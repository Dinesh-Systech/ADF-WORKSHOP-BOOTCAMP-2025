{
	"name": "DF_Tgt_SCD1_Product_IN1675",
	"properties": {
		"folder": {
			"name": "DF_Stg_to_Tgt_IN1675"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_Azure_SQL_IN1675",
						"type": "DatasetReference"
					},
					"name": "SrcProduct"
				},
				{
					"dataset": {
						"referenceName": "DS_Azure_SQL_IN1675",
						"type": "DatasetReference"
					},
					"name": "TgtProduct"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_Azure_SQL_IN1675",
						"type": "DatasetReference"
					},
					"name": "AlcProduct"
				}
			],
			"transformations": [
				{
					"name": "split1"
				},
				{
					"name": "JoinSrcAndTgt"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "derivedColumn2"
				},
				{
					"name": "newRecords"
				},
				{
					"name": "ChangedRecords"
				},
				{
					"name": "union"
				},
				{
					"name": "NullHandling"
				},
				{
					"name": "select6"
				},
				{
					"name": "ProductKey"
				},
				{
					"name": "select7"
				},
				{
					"name": "select8"
				},
				{
					"name": "Transformation"
				}
			],
			"scriptLines": [
				"source(output(",
				"          SKUCode as integer,",
				"          ItemName as string,",
				"          CompanyCode as string,",
				"          IndustryName as string,",
				"          BrandFamily as string,",
				"          ReportingSegment as string,",
				"          BrandPackSize as string,",
				"          PackTypeCode as string,",
				"          BrandPackType as string,",
				"          StateCode as string,",
				"          State as string,",
				"          BrandCode as string,",
				"          BrandName as string,",
				"          ProductCategory as string,",
				"          ProductSegment as string,",
				"          CaseConfig as integer,",
				"          Division as string,",
				"          ConversionFactor as integer,",
				"          AcquiredStartDate as timestamp,",
				"          AcquiredEndDate as timestamp,",
				"          DisposedStartDate as timestamp,",
				"          DisposedEndDate as timestamp,",
				"          InnovationFlag as string,",
				"          Flavour as string,",
				"          SubFlavour as string,",
				"          CreatedBy as string,",
				"          CreatedDate as timestamp,",
				"          UpdatedBy as string,",
				"          UpdatedDate as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SrcProduct",
				"source(output(",
				"          SKUCode as integer,",
				"          Product_Key as integer,",
				"          ItemName as string,",
				"          CompanyCode as string,",
				"          IndustryName as string,",
				"          BrandFamily as string,",
				"          ReportingSegment as string,",
				"          BrandPackSize as string,",
				"          PackTypeCode as string,",
				"          BrandPackType as string,",
				"          StateCode as string,",
				"          State as string,",
				"          BrandCode as string,",
				"          BrandName as string,",
				"          ProductCategory as string,",
				"          ProductSegment as string,",
				"          CaseConfig as integer,",
				"          Division as string,",
				"          ConversionFactor as integer,",
				"          AcquiredStartDate as timestamp,",
				"          AcquiredEndDate as timestamp,",
				"          DisposedStartDate as timestamp,",
				"          DisposedEndDate as timestamp,",
				"          InnovationFlag as string,",
				"          Flavour as string,",
				"          SubFlavour as string,",
				"          CreatedBy as string,",
				"          CreatedDate as timestamp,",
				"          UpdatedBy as string,",
				"          UpdatedDate as timestamp",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> TgtProduct",
				"JoinSrcAndTgt split(isNull(select6@SKUCode),",
				"     !isNull(select6@SKUCode) &&\r",
				"(\r",
				"  (SrcProduct@SKUCode == select6@SKUCode) &&\r",
				"  (\r",
				"    SrcProduct@ReportingSegment!= select6@ReportingSegment\r",
				")\r",
				"),",
				"     disjoint: false) ~> split1@(Insert, Update)",
				"ProductKey, select6 join(SrcProduct@SKUCode == select6@SKUCode,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> JoinSrcAndTgt",
				"split1@Insert derive(CreatedDate = currentTimestamp(),",
				"          CreatedBy = 'IN1675') ~> derivedColumn1",
				"split1@Update derive(UpdatedDate = currentTimestamp(),",
				"          UpdatedBy = 'IN1675') ~> derivedColumn2",
				"derivedColumn1 alterRow(insertIf(true())) ~> newRecords",
				"derivedColumn2 alterRow(updateIf(true())) ~> ChangedRecords",
				"select7 union(byName: true)~> union",
				"Transformation derive(each(match(type=='string'), $$ = iif(isNull($$),'UNK',$$)),",
				"          each(match(type=='integer'), $$ = iifNull($$, -1, $$)),",
				"          each(match(type=='datetime'), $$ = iif(isNull($$), toTimestamp('2999-12-31 00:00:00','yyyy-MM-dd HH:mm:ss'))),",
				"          each(match(type=='timestmap'), $$ = iif(isNull($$), toTimestamp('2999-12-31 00:00:00','yyyy-MM-dd HH:mm:ss')))) ~> NullHandling",
				"TgtProduct select(mapColumn(",
				"          SKUCode,",
				"          Product_Key,",
				"          ItemName,",
				"          CompanyCode,",
				"          IndustryName,",
				"          BrandFamily,",
				"          ReportingSegment,",
				"          BrandPackSize,",
				"          PackTypeCode,",
				"          BrandPackType,",
				"          StateCode,",
				"          State,",
				"          BrandCode,",
				"          BrandName,",
				"          ProductCategory,",
				"          ProductSegment,",
				"          CaseConfig,",
				"          Division,",
				"          ConversionFactor,",
				"          AcquiredStartDate,",
				"          AcquiredEndDate,",
				"          DisposedStartDate,",
				"          DisposedEndDate,",
				"          InnovationFlag,",
				"          Flavour,",
				"          SubFlavour,",
				"          CreatedBy,",
				"          CreatedDate,",
				"          UpdatedBy,",
				"          UpdatedDate",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select6",
				"SrcProduct keyGenerate(output(ProductKey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> ProductKey",
				"newRecords select(mapColumn(",
				"          SKUCode = split1@Insert@SKUCode,",
				"          ProductKey,",
				"          ItemName = split1@Insert@ItemName,",
				"          CompanyCode = split1@Insert@CompanyCode,",
				"          IndustryName = split1@Insert@IndustryName,",
				"          BrandFamily = split1@Insert@BrandFamily,",
				"          ReportingSegment = split1@Insert@ReportingSegment,",
				"          BrandPackSize = split1@Insert@BrandPackSize,",
				"          PackTypeCode = split1@Insert@PackTypeCode,",
				"          BrandPackType = split1@Insert@BrandPackType,",
				"          StateCode = split1@Insert@StateCode,",
				"          State = split1@Insert@State,",
				"          BrandCode = split1@Insert@BrandCode,",
				"          BrandName = split1@Insert@BrandName,",
				"          ProductCategory = split1@Insert@ProductCategory,",
				"          ProductSegment = split1@Insert@ProductSegment,",
				"          CaseConfig = split1@Insert@CaseConfig,",
				"          Division = split1@Insert@Division,",
				"          ConversionFactor = split1@Insert@ConversionFactor,",
				"          AcquiredStartDate = split1@Insert@AcquiredStartDate,",
				"          AcquiredEndDate = split1@Insert@AcquiredEndDate,",
				"          DisposedStartDate = split1@Insert@DisposedStartDate,",
				"          DisposedEndDate = split1@Insert@DisposedEndDate,",
				"          InnovationFlag = split1@Insert@InnovationFlag,",
				"          Flavour = split1@Insert@Flavour,",
				"          SubFlavour = split1@Insert@SubFlavour,",
				"          CreatedBy,",
				"          CreatedDate,",
				"          UpdatedBy = split1@Insert@UpdatedBy,",
				"          UpdatedDate = split1@Insert@UpdatedDate",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select7",
				"ChangedRecords select(mapColumn(",
				"          SKUCode = split1@Update@SKUCode,",
				"          ProductKey,",
				"          ItemName = split1@Update@ItemName,",
				"          CompanyCode = split1@Update@CompanyCode,",
				"          IndustryName = split1@Update@IndustryName,",
				"          BrandFamily = split1@Update@BrandFamily,",
				"          ReportingSegment = split1@Update@ReportingSegment,",
				"          BrandPackSize = split1@Update@BrandPackSize,",
				"          PackTypeCode = split1@Update@PackTypeCode,",
				"          BrandPackType = split1@Update@BrandPackType,",
				"          StateCode = split1@Update@StateCode,",
				"          State = split1@Update@State,",
				"          BrandCode = split1@Update@BrandCode,",
				"          BrandName = split1@Update@BrandName,",
				"          ProductCategory = split1@Update@ProductCategory,",
				"          ProductSegment = split1@Update@ProductSegment,",
				"          CaseConfig = split1@Update@CaseConfig,",
				"          Division = split1@Update@Division,",
				"          ConversionFactor = split1@Update@ConversionFactor,",
				"          AcquiredStartDate = split1@Update@AcquiredStartDate,",
				"          AcquiredEndDate = split1@Update@AcquiredEndDate,",
				"          DisposedStartDate = split1@Update@DisposedStartDate,",
				"          DisposedEndDate = split1@Update@DisposedEndDate,",
				"          InnovationFlag = split1@Update@InnovationFlag,",
				"          Flavour = split1@Update@Flavour,",
				"          SubFlavour = split1@Update@SubFlavour,",
				"          CreatedBy = split1@Update@CreatedBy,",
				"          CreatedDate = split1@Update@CreatedDate,",
				"          UpdatedBy,",
				"          UpdatedDate",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select8",
				"union derive(ItemName = concat(split(ItemName, '-')[1], '-', Flavour, '-', split(ItemName, '-')[3])) ~> Transformation",
				"NullHandling sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> AlcProduct"
			]
		}
	}
}