{
	"name": "DF_Sales_Org_Cluster_STG_TGT_IN1686",
	"properties": {
		"folder": {
			"name": "Final_Project/STG_TGT"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_AzureSqlTable_TGT_IN1686",
						"type": "DatasetReference"
					},
					"name": "SRCSalesOrgCluster"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_AzureSqlTable_TGT_IN1686",
						"type": "DatasetReference"
					},
					"name": "SNKAlcSalesOrgCluster"
				}
			],
			"transformations": [
				{
					"name": "SKSalesOrgCluster"
				},
				{
					"name": "NullHandling"
				},
				{
					"name": "select1"
				},
				{
					"name": "DataTypeConversion"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Cluster Name} as string,",
				"          {Cluster Code} as string,",
				"          {Cluster ID} as integer,",
				"          {Cluster Head} as string,",
				"          {Cluster SFA Code} as string,",
				"          {Cluster Head Email} as string,",
				"          {Cluster Head Mobile} as long,",
				"          Active_flag as string,",
				"          {Cluster Start Date} as string,",
				"          {Cluster End Date} as string,",
				"          {Created By} as string,",
				"          {Created Date} as timestamp,",
				"          {Updated By} as string,",
				"          {Updated Date} as timestamp,",
				"          Pipeline_RunID as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SRCSalesOrgCluster",
				"NullHandling keyGenerate(output(ClusterKey as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> SKSalesOrgCluster",
				"DataTypeConversion derive(each(match(type=='integer'), $$ = iifNull($#,0)),",
				"          each(match(type=='string'), $$ = replace(iifNull($$,'UNK'),'null','UNK')),",
				"          each(match(type=='timestamp'), $$ = iifNull($$,toTimestamp('31/12/2999 00:00:00'))),",
				"          each(match(type=='date'), $$ = iifNull($$,toDate('31/12/2999')))) ~> NullHandling",
				"SKSalesOrgCluster select(mapColumn(",
				"          ClusterKey,",
				"          {Cluster Name},",
				"          {Cluster Code},",
				"          {Cluster ID},",
				"          {Cluster Head},",
				"          {Cluster SFA Code},",
				"          {Cluster Head Email},",
				"          {Cluster End Date},",
				"          {Cluster Head Mobile},",
				"          Active_flag,",
				"          {Cluster Start Date},",
				"          {Created By},",
				"          {Created Date},",
				"          {Updated By},",
				"          {Updated Date},",
				"          Pipeline_RunID",
				"     ),",
				"     partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"SRCSalesOrgCluster derive({Cluster Start Date} = toDate({Cluster Start Date},'dd-MM-yyyy'),",
				"          {Cluster End Date} = toDate({Cluster End Date},'dd-MM-yyyy')) ~> DataTypeConversion",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          PromotionCode as string,",
				"          PromotionDescription as string,",
				"          PromotionType as string,",
				"          RetailerCode as string,",
				"          EffectiveFrom as date,",
				"          EffectiveTo as date,",
				"          MappingLevel as string,",
				"          IsActive as string,",
				"          {Created By} as string,",
				"          {Created Date} as timestamp,",
				"          {Updated By} as string,",
				"          {Updated Date} as timestamp",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> SNKAlcSalesOrgCluster"
			]
		}
	}
}