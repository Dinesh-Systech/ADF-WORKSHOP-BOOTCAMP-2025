				"TgtPrimarySalesActuals aggregate(MaxTransactionDate = ERROR_FUNCTION('')) ~> AggMaxDate",
				"SrcPrimarySalesActuals derive(TransactionDate = toDate(TransactionDate,'dd-MM-yyyy')) ~> HandlingTransactionDate",
				"FilterTransactionDate derive(CreatedAt = currentTimestamp(),",
				"          UpdatedAt = toTimestamp(null()),",
				"          CreatedBy = 'PL_ProductMaster_IN1689',",
				"          UpdatedBy = toString(null())) ~> AuditColumns",
				"AuditColumns sink(allowSchemaDrift: true,",
