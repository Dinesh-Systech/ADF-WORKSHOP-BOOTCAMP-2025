				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          CompanyCode,",
				"          SalesOrgCode,",
				"          DistributionChannel,",
				"          Division,",
				"          CustomerCode,",
				"          CustomerName,",
				"          CountryCode,",
				"          ZoneCode,",
				"          StateCode,",
				"          SKUCode,",
				"          PlantCode,",
				"          TransactionDate,",
				"          VolumeActualCase,",
				"          CreatedAt,",
				"          UpdatedAt,",
				"          CreatedBy,",
				"          UpdatedBy",
